  "rawText": " async (\n  persona: Persona,\n  voice: string,\n  text: string[] | string,\n  speed?: number,\n  quality?: \"low\" | \"medium\" | \"high\" | \"premium\"\n): Promise<string | undefined> => {\n  const result = await playhtRequest(persona, \"POST:tts\", {\n    output_format: \"wav\",\n    sample_rate: 24000,\n    text,\n    voice,\n    quality,\n    speed,\n  });\n\n  return result?.url;\n}",
